	<queryString>
		<![CDATA[select ROW_NUMBER() OVER(ORDER BY par.id DESC) AS rownum, p.nombres as nombresEst, p.apellidos as apellidosEst, par.nombre as nombreParalelo, par.id as idPar, m.id as nummat, p.cedula as cedula, p.fecha_nacimiento as fechanac, p.sexo as sexo

from usuarios.persona as p
inner join usuarios.estudiante e
on p.id = e.persona
inner join colegios.matricula m
on e.id = m.estudiante
inner join colegios.paralelo par
on par.id = m.paralelo
where m.asignacion_curso = $P{idAsigCur}
order by par.id asc]]>
	</queryString>
