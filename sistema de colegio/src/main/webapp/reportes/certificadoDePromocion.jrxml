	<queryString>
		<![CDATA[select det_mat.id, mat.nombre, det_mat.total_prom, det_mat.total_prom_string
from colegios.materia as mat
inner join colegios.detalle_materia as det_mat
on mat.id = det_mat.materia
inner join colegios.asignacion_curso_materias as acm
on mat.id = acm.materia
where det_mat.estudiante = $P{idEst}
and acm.asignacion_curso = $P{idAsigCurso}
and acm.paralelo = $P{idParalelo}]]>
